type Order {
    id: String!
    Price: Float!
    Tax: Float!
    FinalPrice: Float!
}

input OrderInput {
    id : String!
    Price: Float!
    Tax: Float!
}

input ListOrdersInput {
    Page: Int!
    Limit: Int!
}

type Mutation {
    createOrder(input: OrderInput): Order
}

type Query {
    listOrders(input: ListOrdersInput): [Order]
}